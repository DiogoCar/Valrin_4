package exercicio_fix;

import javax.swing.JOptionPane;
/**
 *
 * @author Discente
 */

public class Valrin {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here

        int n = 5;
        int[][] A = new int[n][n];
        int[][] B = new int[n][n];
        int[][] C = new int[n][n];

        // Preencha as matrizes A e B aqui

        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                if (Arrays.stream(B).flatMapToInt(Arrays::stream).anyMatch(x -> x == A[i][j])) {
                    C[i][j] = 1;
                } else {
                    C[i][j] = 0;
                }
            }
        }

        // Imprima a matriz C aqui
    }
}
